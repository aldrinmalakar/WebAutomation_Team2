<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestSuit">

    <parameter name="useCloudEnv" value="true"/>
    <parameter name="OS" value="catalina"/>
    <parameter name="OS" value="Windows"/>
    <!--        <parameter name="os_version" value="###"/>-->
    <parameter name="os_version" value="Sierra"/>
    <parameter name="cloudEnvName" value="browserStack"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="86"/>
    <parameter name="url" value="https://www.walmartmuseum.com/content/walmartmuseum/en_us.html"/>
    <test name="Walmart Digital Museum">
        <classes>
            <class name="walmarttesting.MuseumTest">
                <methods>
                    <exclude name="ScrollToExploreFunctions"></exclude>
<!--                    <exclude name="testSearchBoxCheck1"></exclude>-->
<!--                    <include name="testSearchBoxCheck2"></include>-->
                </methods>
            </class>
        </classes>
    </test>


</suite>




